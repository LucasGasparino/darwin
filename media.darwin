title  "Media e operadores aritmeticos";
variables
    sample of float amostra;
    float i;
    float j;
    string k;
    string w;
end variables
commands
    amostra := {12.0,32.0,34.0,53.0};
    i := mean(amostra);
    j := 3.0 * (i + 10.0) - 1.0;
    k := toString(j);
    print(k);
    w := toString((i / (3.0 + 1.0)));
    print(w);
