title "Controlando a variancia de uma amostra de porcentagens";
variables
    sample of float porcentagens;
    float variancia;
end variables
commands
    porcentagens := {12.4,43.5,53.3,93.4};
    variancia := variance(porcentagens);
    if(variancia > 50.0) then
        print("A variancia esta muito alta");
    else
        print("A variancia esta de acordo");
    end