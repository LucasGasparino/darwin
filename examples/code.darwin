title "Operadores Logicos";
variables
    boolean igual;
    boolean diferente;
    boolean maior;
    boolean maior;
    boolean maior_igual;
    boolean menor_igual;
end variables
commands
    igual := (1 == 3);
    diferente := (1 != 3);
    maior := (1 > 3);
    maior := (1 < 3);
    maior_igual := (1 >= 3);
    menor_igual := (1 <= 3);
